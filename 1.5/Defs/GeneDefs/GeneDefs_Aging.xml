<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<GeneDef>
		<defName>VRE_EarlyMaturity</defName>
		<label>early maturity</label>
		<description>Carriers of this gene reach skill maturity significantly earlier than others. They can do majority of tasks at the age of 4, and can do wardening, doctoring and construction at the age of 7.</description>
		<iconPath>UI/Icons/Genes/Gene_EarlyMaturity</iconPath>
		<displayCategory>Reproduction</displayCategory>
		<displayOrderInCategory>500</displayOrderInCategory>
		<biostatMet>-1</biostatMet>
		<biostatCpx>1</biostatCpx>
		<modExtensions>
			<li Class="VREArchon.LifeStageWorkSettingsExtension">
				<lifeStageWorkSettings>
					<Firefighter>4</Firefighter>
					<Patient>0</Patient>
					<Doctor>7</Doctor>
					<PatientBedRest>0</PatientBedRest>
					<Childcare MayRequire="Ludeon.RimWorld.Biotech">0</Childcare>
					<BasicWorker>3</BasicWorker>
					<Warden>7</Warden>
					<Handling>4</Handling>
					<Cooking>4</Cooking>
					<Hunting>4</Hunting>
					<Construction>7</Construction>
					<Growing>4</Growing>
					<Mining>4</Mining>
					<PlantCutting>4</PlantCutting>
					<Smithing>10</Smithing>
					<Tailoring>4</Tailoring>
					<Art>7</Art>
					<Hauling>3</Hauling>
					<Cleaning>3</Cleaning>
					<Research>10</Research>
				</lifeStageWorkSettings>
			</li>
		</modExtensions>
	</GeneDef>
	
	
</Defs>