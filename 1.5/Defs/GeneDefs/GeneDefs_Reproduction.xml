<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<GeneDef>
		<defName>VRE_LongPregnancy</defName>
		<label>long pregnancy</label>
		<exclusionTags>
			<li>AG_Gestation</li>
			<li>AG_Fertility</li>
		</exclusionTags>
		<description>Carriers of this gene take twice as long to develop a child during pregnancy. Pregnancy lasts three hexamesters - 36 days.</description>
		<iconPath>UI/Icons/Genes/Gene_PregnancyLong</iconPath>
		<displayCategory>Reproduction</displayCategory>
		<displayOrderInCategory>115.2</displayOrderInCategory>
		<biostatMet>1</biostatMet>
		<biostatCpx>1</biostatCpx>
		
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<pregnancySpeedFactor>0.5</pregnancySpeedFactor>
			</li>
		</modExtensions>
	</GeneDef>
	<GeneDef>
		<defName>VRE_ShortPregnancy</defName>
		<label>short pregnancy</label>
		<exclusionTags>
			<li>AG_Gestation</li>
			<li>AG_Fertility</li>
		</exclusionTags>
		<description>Carriers of this gene develop a child during pregnancy much faster. Pregnancy lasts 3 bimesters - 12 days.</description>
		<iconPath>UI/Icons/Genes/Gene_PregnancyShort</iconPath>
		<displayCategory>Reproduction</displayCategory>
		<displayOrderInCategory>115.3</displayOrderInCategory>
		<biostatMet>-1</biostatMet>
		<biostatCpx>1</biostatCpx>
		
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<pregnancySpeedFactor>1.5</pregnancySpeedFactor>
			</li>
		</modExtensions>
	</GeneDef>
	
</Defs>